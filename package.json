{
  "name": "typescript-aniskip-api",
  "version": "1.2.1",
  "description": "Provides the opening and ending skip times for the Aniskip extension",
  "main": "src/app.ts",
  "repository": "https://github.com/lexesjan/typescript-aniskip-api",
  "contributors": [
    "Lexes Jan Mantiquilla <mantiquillal@gmail.com>",
    "Dabreadman <tany3@tcd.ie>"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc --project ./",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development nodemon --exec 'yarn ts-node' --legacy-watch src/main.ts",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "test": "jest --coverage ./src"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/platform-express": "^8.0.6",
    "class-validator": "^0.13.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "packageManager": "yarn@3.0.1"
